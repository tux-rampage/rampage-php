{
	"name" : "rampage-php/framework",
	"description" : "Rampage PHP Framework based on Zend Framework 2",
	"type" : "library",
	"authors" : [{
			"name" : "Axel H.",
			"email" : "rampagephp@linux-rampage.org",
			"role" : "developer"
		}
	],
	"keywords" : [
		"framework",
		"zf2",
		"rampage"
	],
	"homepage" : "https://github.com/tux-rampage/rampage-php",
	"license" : [
		"GPL-3.0"
	],
	"require" : {
		"php" : ">= 5.3.3",
		"zendframework/zendframework" : "~2.1.0"
	},
	"autoload" : {
		"psr-0" : {
			"rampage" : "library/"
		}
	},
	"support" : {
		"source" : "https://github.com/tux-rampage/rampage-php",
		"issues" : "https://github.com/tux-rampage/rampage-php/issues",
		"wiki" : "https://github.com/tux-rampage/rampage-php/wiki"
	},
	"repositories" : [{
			"type" : "composer",
			"url" : "https://packages.zendframework.com/"
		}
	],
	"bin" : [
		"tools/generate-di.php"
	],
	"suggest" : {
		"rampage-php/dev-tools" : "Integration of DB profiling for Zend Developer Tools",
		"rampage-php/composer-module-installer" : "Allow installing application modules via composer"
	}
}