{
  "name" : "rampage-php/framework",
  "description" : "Rampage PHP Framework based on Zend Framework 2",
  "require" : {
    "php" : ">= 5.3.3",
    "zendframework/zend-mvc" : ">= 2.2",
    "zendframework/zend-di" : ">= 2.2",
    "zendframework/zend-i18n" : ">= 2.2",
    "zendframework/zend-modulemanager" : ">= 2.2",
    "zendframework/zend-authentication" : ">= 2.2",
    "zendframework/zend-config" : ">= 2.2",
    "zendframework/zend-view" : ">=2.2"
  },
  "require-dev" : {
    "zendframework/zend-code" : ">= 2.2",
    "phpunit/phpunit" : "3.7.*",
    "doctrine/common" : "~2.4"
  },
  "authors" : [ {
    "name" : "Axel H.",
    "email" : "rampagephp@linux-rampage.org",
    "role" : "developer"
  } ],
  "support" : {
    "source" : "https://github.com/tux-rampage/rampage-php",
    "issues" : "https://github.com/tux-rampage/rampage-php/issues",
    "wiki" : "https://github.com/tux-rampage/rampage-php/wiki"
  },
  "keywords" : [ "framework", "zf2", "rampage" ],
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0.x-dev"
    }
  },
  "autoload" : {
    "psr-0" : {
      "rampage\\" : "library"
    },
    "classmap" : [ "library/" ]
  },
  "suggest" : {
    "zendframework/zend-json" : "ZF2 JSON en-/decoding",
    "zendframework/zend-console" : "ZF2 Console handling",
    "zendframework/session" : "ZF2 Session Handling"
  },
  "bin" : [ "tools/generate-di.php", "tools/rampage-app-skeleton.php" ],
  "license" : "GPL-3.0",
  "type" : "library",
  "homepage" : "https://github.com/tux-rampage/rampage-php"
}