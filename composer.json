{
  "name" : "rampage-php/framework",
  "description" : "Rampage PHP Framework based on Zend Framework 2",
  "require" : {
    "php" : ">= 5.5",
    "zendframework/zend-mvc" : "2.*",
    "zendframework/zend-di" : "2.*",
    "zendframework/zend-i18n" : "2.*",
    "zendframework/zend-modulemanager" : "2.*",
    "zendframework/zend-config" : "2.*",
    "zendframework/zend-view" : "2.*",
    "zendframework/zend-json" : "2.*",
    "zendframework/zend-console" : "2.*",
    "zendframework/zend-log" : "2.*",
    "zendframework/zend-http" : "2.*",
    "zendframework/zend-text" : "2.2.*",
    "zendframework/zend-servicemanager" : "2.*"
  },
  "require-dev" : {
    "zendframework/zend-code" : "2.*",
    "phpunit/phpunit" : "3.7.*",
    "doctrine/common" : "~2.4",
    "zendframework/zend-form" : "2.*",
    "zendframework/zend-serializer" : "2.*"
  },
  "license" : "GPL-3.0",
  "keywords" : [ "framework", "zf2", "rampage" ],
  "bin" : [ "tools/rampage-app-skeleton.php" ],
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.2.x-dev"
    }
  },
  "autoload" : {
    "psr-0" : {
      "rampage\\" : "library"
    },
    "classmap" : [ "library/" ]
  },
  "suggest" : {
    "zendframework/zend-console" : "ZF2 Console handling",
    "zendframework/zend-session" : "ZF2 Session Handling",
    "rampage-php/auth" : "Authentication framework for rampage-php"
  },
  "type" : "library",
  "support" : {
    "source" : "https://github.com/tux-rampage/rampage-php",
    "issues" : "https://github.com/tux-rampage/rampage-php/issues",
    "wiki" : "https://github.com/tux-rampage/rampage-php/wiki"
  },
  "authors" : [ {
    "name" : "Axel H.",
    "email" : "rampagephp@linux-rampage.org",
    "role" : "developer"
  } ],
  "homepage" : "https://github.com/tux-rampage/rampage-php"
}